{
  "name": "micro-front-ends",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "shell": "ng serve shell --configuration development --port 4200 -o",
    "remote": "ng serve remote --configuration development --port 4300 -o",
    "start": "concurrently \"npm run shell\" \"npm run remote\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
